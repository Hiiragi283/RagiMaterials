{
  "credit": "Tweaked by Hiiragi Russell Tsubasa",
  "multipart": [
    {
      "apply": {
        "model": "ragi_materials:salt_pond_base"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_wall"
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_wall",
        "y": 90
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_wall",
        "y": 180
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_wall",
        "y": 270
      }
    },
    {
      "when": {
        "type": "water"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_water"
      }
    },
    {
      "when": {
        "type": "saltwater"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_saltwater"
      }
    },
    {
      "when": {
        "type": "brine"
      },
      "apply": {
        "model": "ragi_materials:salt_pond_brine"
      }
    }
  ]
}